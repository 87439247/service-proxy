<proxies xmlns="http://membrane-soa.org/proxies/1/"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      	 xsi:schemaLocation="http://membrane-soa.org/proxies/1/ http://membrane-soa.org/schemas/proxies-1.xsd">
	<serviceProxy name="localhost" host="*" port="2000"
		method="*">
		<interceptor refid="accessControlInterceptor" />
		<target host="localhost" port="3011" />
	</serviceProxy>
	<proxy name="proxy" port="2001">
		<interceptor refid="accessControlInterceptor" />
	</proxy>
</proxies>
