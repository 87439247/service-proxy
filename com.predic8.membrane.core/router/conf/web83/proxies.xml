<proxies xmlns="http://membrane-soa.org/schemas/proxies/v1/"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      	 xsi:schemaLocation="http://membrane-soa.org/schemas/proxies/v1/ http://membrane-soa.org/schemas/proxies/v1/proxies.xsd">
      	 
	<serviceProxy name="Console" port="9083">
		<basicAuthentication>
			<user name="admin" password="membrane" />
		</basicAuthentication>	
		<adminConsole />
	</serviceProxy>	
	
	<serviceProxy name="CarResource" port="83">
		<regExUrlRewriter>
			<mapping regex="^$" uri="/echo-webapp/" />
			<mapping regex="/$" uri="/echo-webapp/" />
		</regExUrlRewriter>	
		<request>
			<validator jsonSchema="C:/work/workspace/com.predic8.membrane.core/examples/router/validation/json-schema/schema2001.json" />
		</request>
		<target host="localhost" port="8080" />
	</serviceProxy>
	
</proxies>
