<rules>
  <forwardingRule name="Administration" port="9000">
    <target host="localhost" port="8080" />
    <interceptors>
		<adminConsole />
		<webServer docBase="docBase" />
    </interceptors>
  </forwardingRule>
  <forwardingRule name="Balancer" port="8080">
    <target host="localhost" port="8080" />
    <interceptors>
		<balancer>
			<xmlSessionIdExtractor namespace="http://chat.predic8.com/" localName="session" />
		</balancer>
    </interceptors>
  </forwardingRule>
</rules>
