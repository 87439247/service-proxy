<rules>
	<forwardingRule name="Administration" port="9000">
		<target host="localhost" port="8080" />
		<interceptors>
			<basicAuthentication>
				<user name="admin" password="admin" />
			</basicAuthentication>
			<adminConsole />
			<webServer docBase="docBase" />
		</interceptors>
	</forwardingRule>
	<forwardingRule name="Node 1" port="4000">
		<target host="localhost" port="8080" />
		<interceptors>
			<counter name="Node 1" />
		</interceptors>
	</forwardingRule>
	<forwardingRule name="Node 2" port="4001">
		<target host="localhost" port="8080" />
		<interceptors>
			<counter name="Node 2" />
		</interceptors>
	</forwardingRule>
	<forwardingRule name="Node 3" port="4002">
		<target host="localhost" port="8080" />
		<interceptors>
			<counter name="Node 3" />
		</interceptors>
	</forwardingRule>
	<forwardingRule name="Push Interface" port="9010">
		<target host="localhost" port="8080" />
		<interceptors>
			<clusterNotification validateSignature="true"
				keyHex="6f488a642b740fb70c5250987a284dc0" />
		</interceptors>
	</forwardingRule>
	<forwardingRule name="Balancer" port="8080">
		<target host="localhost" port="8080" />
		<interceptors>
			<balancer />
		</interceptors>
	</forwardingRule>
</rules>
