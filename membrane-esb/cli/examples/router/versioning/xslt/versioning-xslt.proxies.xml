<proxies xmlns="http://membrane-soa.org/schemas/proxies/v1/"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      	 xsi:schemaLocation="http://membrane-soa.org/schemas/proxies/v1/ http://membrane-soa.org/schemas/proxies/v1/proxies.xsd">

	<serviceProxy port="80" name="ContactService">
		<switch>
			<case xPath="//*[namespace-uri()='http://predic8.com/contactService/v11']" service="v11-to-v20" />
			<case xPath="//*[namespace-uri()='http://predic8.com/contactService/v20']" url="http://localhost:8080/ContactService/v20"/>
		</switch>
	</serviceProxy>
	
	<serviceProxy port="80" name="v11-to-v20">
		<request>
			<transform xslt="v11-to-v20.xslt"/>
		</request>
		<target url="http://localhost:8080/ContactService/v20" />
	</serviceProxy>

</proxies>