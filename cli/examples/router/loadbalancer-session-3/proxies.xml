<proxies xmlns="http://membrane-soa.org/proxies/1/"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      	 xsi:schemaLocation="http://membrane-soa.org/proxies/1/ http://membrane-soa.org/schemas/proxies-1.xsd">
	<serviceProxy name="Balancer" port="8080">
		<balancer sessionTimeout="3600000">    <!-- timeout is 1 hour -->
			<xmlSessionIdExtractor namespace="http://chat.predic8.com/"
				localName="session" />
		</balancer>
	</serviceProxy>
	<serviceProxy name="Administration" port="9000">
		<adminConsole />
	</serviceProxy>
</proxies>
